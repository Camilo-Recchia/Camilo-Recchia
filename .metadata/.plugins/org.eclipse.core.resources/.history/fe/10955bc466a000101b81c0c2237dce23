package pruebaIf;

import java.awt.datatransfer.SystemFlavorMap;
import java.util.Scanner;

public class PruebaIF {

	public static void main(String[] args) {
		
		Scanner teclado = new Scanner(System.in);
		
		int calidad;
		System.out.print("Dime del 1 al 10 que tan bien fue tu experiencia: ");
		calidad = teclado.nextInt();
		
		if (calidad == 10) {
			System.out.print("Enorabuena, gracias por venir");
			
			
		}
		
		else {
			System.out.print("Hoy dormi con un ojo abierto");
			
			
		}
		
	}

}
